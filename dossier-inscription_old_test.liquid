                        {% guest_checkbox_field stage_bugatti_alpine "Stage Bugatti Alpine", show_if: "stage_bugatti==1"  %}
                        {% guest_checkbox_field stage_bugatti_lamborghini "Stage Bugatti Lamborghini", show_if: "stage_bugatti==1"  %}
